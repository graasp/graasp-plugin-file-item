{
  "name": "graasp-plugin-file-item",
  "version": "0.1.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "description": "FileItem fastify plugin for Graasp",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "prepack": "tsc",
    "prepare": "tsc",
    "test": "jest --forceExit --coverage --verbose",
    "test:watch": "yarn test --watchAll",
    "prettier:check": "prettier --check \"{src,test}/**/*.ts\"",
    "prettier:write": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --fix"
  },
  "license": "MIT",
  "some-comments": [
    "fastify-error: ~0.2.0"
  ],
  "dependencies": {
    "graasp-file-upload-limiter": "github:graasp/graasp-file-upload-limiter",
    "graasp-plugin-file": "github:graasp/graasp-plugin-file",
    "graasp-plugin-public": "github:graasp/graasp-plugin-public"
  },
  "devDependencies": {
    "@types/content-disposition": "0.5.4",
    "@types/eslint": "^7.2.5",
    "@types/graasp": "github:graasp/graasp-types",
    "@types/jest": "27.0.1",
    "@types/node": "16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "fastify": "3.8.0",
    "graasp-test": "github:graasp/graasp-test",
    "jest": "27.2.0",
    "prettier": "2.4.1",
    "ts-jest": "27.0.5",
    "typescript": "4.4.2",
    "uuid": "8.3.2"
  },
  "packageManager": "yarn@3.2.0-rc.3"
}
