{
  "name": "graasp-plugin-file-item",
  "version": "0.1.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "description": "FileItem fastify plugin for Graasp",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "prepack": "tsc",
    "prepare": "tsc",
    "test": "jest --forceExit --coverage --verbose",
    "test:watch": "yarn test --watchAll",
    "prettier:check": "prettier --check \"{src,test}/**/*.ts\"",
    "prettier:write": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "tsc --noEmit && eslint \"{src,test}/**/*.{js,ts}\" --fix",
    "pre-commit": "yarn prettier:check && yarn lint",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "post-commit": "git status"
  },
  "license": "MIT",
  "dependencies": {
    "graasp-file-upload-limiter": "github:graasp/graasp-file-upload-limiter",
    "graasp-plugin-file": "github:graasp/graasp-plugin-file#29/sdk",
    "graasp-plugin-public": "github:graasp/graasp-plugin-public#44/sdk"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.4",
    "@commitlint/config-conventional": "16.2.4",
    "@graasp/sdk": "github:graasp/graasp-utils#15/testUtils",
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/content-disposition": "0.5.4",
    "@types/eslint": "8.4.1",
    "@types/jest": "27.5.0",
    "@types/node": "17.0.31",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "eslint": "8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "fastify": "^3.29.1",
    "graasp-test": "github:graasp/graasp-test#23/sdk",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "prettier": "2.6.2",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "typescript": "4.6.4",
    "uuid": "8.3.2"
  },
  "packageManager": "yarn@3.2.1"
}
